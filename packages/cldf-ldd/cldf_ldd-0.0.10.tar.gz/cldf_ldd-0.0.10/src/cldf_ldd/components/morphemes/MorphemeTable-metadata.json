{
    "url": "morphemes.csv",
    "tableSchema": {
        "columns": [
            {
                "name": "ID",
                "required": true,
                "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#id",
                "datatype": {
                    "base": "string",
                    "format": "[a-zA-Z0-9_\\-]+"
                }
            },
            {
                "name": "Language_ID",
                "required": true,
                "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#languageReference",
                "dc:extent": "singlevalued",
                "dc:description": "A reference to a language (or variety) the morpheme belongs to.",
                "datatype": "string"
            },
            {
                "name": "Name",
                "required": true,
                "dc:extent": "singlevalued",
                "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#name",
                "datatype": "string"
            },
            {
                "name": "Description",
                "dc:extent": "singlevalued",
                "dc:description": "A human-readable description.",
                "required": false,
                "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#description",
                "datatype": "string"
            },
            {
                "name": "Parameter_ID",
                "required": false,
                "dc:description": "A reference to the meaning denoted by the morpheme.",
                "dc:extent": "multivalued",
                "datatype": "string",
                "separator": "; "
            },
            {
                "name": "Comment",
                "required": false,
                "dc:extent": "singlevalued",
                "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#comment",
                "datatype": "string"
            },
            {
                "name": "Source",
                "required": false,
                "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#source",
                "datatype": "string",
                "separator": "; "
            },
            {
                "name": "Part_Of_Speech",
                "required": false,
                "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#partOfSpeech",
                "datatype": "string",
                "dc:extent": "singlevalued"
            }
        ]
    }
}