<odoo>
<data noupdate="1">
<record id="hr_cash_advance_sequence" model="ir.sequence">
    <field name="name">Employee Cash Advance</field>
    <field name="code">hr.cash_advance</field>
    <field name="prefix">CA/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

<record id="hr_cash_advance_settlement_sequence" model="ir.sequence">
    <field name="name">Employee Cash Advance Settlement</field>
    <field name="code">hr.cash_advance_settlement</field>
    <field name="prefix">CAS/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
</data>
</odoo>
