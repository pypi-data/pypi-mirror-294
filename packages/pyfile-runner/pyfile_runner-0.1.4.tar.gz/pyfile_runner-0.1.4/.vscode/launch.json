    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Run+Debug: with_ui.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/example/with_ui.py",
            "console": "externalTerminal"
        },
        {
            "name": "Run+Debug: plain.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/example/plain.py",
            "console": "integratedTerminal"
        }
    ]
}