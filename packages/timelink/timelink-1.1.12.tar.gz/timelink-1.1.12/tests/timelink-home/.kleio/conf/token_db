created(1725525975.6866682).
assert(user_token('619fb8f4c7e1e21d3d23e41b3c396c4a636c0fae',limited_user,[created(1725525975.6807463),comment('An user that has no privileges, used to test authorization errors'),api([]),structures('projects/test-project/structures'),sources('projects/test-project/sources')])).
assert(user_token('588e30542cdc68eb333c1c3a3dbb50190ed02e99',normal_user,[created(1725525975.7650595),comment('An user able to translate, upload and delete files, and also create and remove directories, in specific sub-directories in kleio-home'),api([sources,kleioset,files,structures,translations,upload,delete,mkdir,rmdir]),structures('projects/test-project/structures'),sources('projects/test-project/sources')])).
