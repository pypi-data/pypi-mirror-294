<data noupdate="1">
<record id="sequence_loan_out" model="ir.sequence">
    <field name="name">Default Loan Out</field>
    <field name="code">loan.out</field>
    <field name="prefix">LO/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

<record id="sequence_loan_in" model="ir.sequence">
    <field name="name">Default Loan In</field>
    <field name="code">loan.in</field>
    <field name="prefix">LI/%(range_year)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>
</data>
</odoo>
